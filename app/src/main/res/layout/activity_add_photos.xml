<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".AddPhotosActivity">

    <com.google.android.material.imageview.ShapeableImageView
        android:id="@+id/add_photos2"
        android:layout_width="140dp"
        android:layout_marginLeft="8dp"
        android:layout_height="138dp"
        app:strokeWidth="2dp"
        android:src="@drawable/ic_add_fill0_wght400_grad0_opsz48"
        app:layout_constraintStart_toStartOf="parent"

        app:layout_constraintTop_toTopOf="parent" />
    <Button
        android:id="@+id/add_Photos_finish"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:icon="@drawable/ic_done_fill0_wght400_grad0_opsz48"
        android:layout_marginRight="130dp"
        android:layout_marginTop="500dp"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/add_finish"
        style="@style/Widget.MaterialComponents.Button.Icon"
        />


</androidx.constraintlayout.widget.ConstraintLayout>